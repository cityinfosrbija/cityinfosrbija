<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mzbcacak.kele.cityinfosrbija.zaParking">

    <fragment
        android:id="@+id/headerP"
        android:name="com.mzbcacak.kele.cityinfosrbija.HeaderFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        tools:layout="@layout/fragment_header" />

    <fragment
        android:id="@+id/footerP"
        android:name="com.mzbcacak.kele.cityinfosrbija.FooterFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        tools:layout="@layout/fragment_footer" />

    <fragment
        android:id="@+id/mapaParking"
        android:name="com.mzbcacak.kele.cityinfosrbija.MapFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/headerP"
        android:layout_above="@+id/footerP"
        tools:layout="@layout/fragment_map" />
</RelativeLayout>
